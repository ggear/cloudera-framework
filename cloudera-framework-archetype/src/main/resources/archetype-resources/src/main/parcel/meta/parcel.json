{
	"schema_version": 1,
	"name": "${parcel.name}",
	"version": "${parcel.version}",
	"extraVersionInfo": {
		"fullVersion": "${parcel.version.full}",
		"baseVersion": "${parcel.version.base}"
	},
	"setActiveSymlink": true,
	"scripts": {
		"defines": "${parcel.label}-env.sh"
	},
	"packages": [{
		"name": "${parcel.label}",
		"version": "${parcel.version}"
	}],
	"components": [{
		"name": "${parcel.label}",
		"version": "${parcel.version}",
		"pkg_version": "${parcel.version}"
	}],
	"provides": [
		"flume-plugin",
		"hive-plugin",
		"yarn-plugin",
		"${parcel.label}"
	],
	"users": {
		"${parcel.namespace}": {
			"longname": "${parcel.label}",
			"home": "/var/lib/${parcel.label}",
			"shell": "/bin/false",
			"extra_groups": []
		}
	},
	"groups": [
		"hadoop"
	]
}